{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "lint": {
      "cache": false
    },
    "e2e": {
      "persistent": true,
      "cache": false
    },
    "e2e:build": {
      "cache": true,
      "dependsOn": ["db:reset", "build"],
      "dotEnv": [".env.test"]
    },
    "start": {
      "dependsOn": ["build"],
      "cache": true,
      "persistent": true
    },
    "start:prod": {
      "cache": true,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"],
      "dotEnv": [".env"]
    },
    "db:push": {
      "cache": false
    },
    "db:studio": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    },
    "db:reset": {
      "cache": false
    },
    "typecheck": {
      "cache": false
    },
    "test": {},
    "test:watch": {
      "cache": false
    },
    "clear": {
      "cache": false
    }
  }
}
